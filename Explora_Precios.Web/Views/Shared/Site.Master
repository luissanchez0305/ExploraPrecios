	<script type="text/javascript">
		var groupShowing = false;
		$(".groups").simpletip({
			content: '',
			fixed: true,
			position: ["-1200", "-10"],
			onShow: function () {
				$("#groupLoading").show();	
				if (!groupShowing) {
					var simpleTip = this;
					$.get('<%= Url.Action("GetGroupManager", "Home") %>', function (data) {
						simpleTip.update(data.html);
						$("#groupLoading").hide();
					});
					groupShowing = true;
				}
			},
